Export
'' Type Font  Roman

Public Function Roman(sString As String) As String 
  
  Return "{\\rm " & sString & "}"
  
End
'' Type Font  Boldface

Public Function Boldface(sString As String) As String
  
  Return "{\\bf " & sString & "}"
  
End
