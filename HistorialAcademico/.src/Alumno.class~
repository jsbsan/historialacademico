    SQlString &= " WHERE FechaNacimiento BETWEEN " & valorMin & " And " & valorMax & "Order by CampoOrden"
  Endif 
  Try mRs = mBase.Exec(SQLString) 
  If Error Then 
    Message.Error(" Se ha producido un ERROR al Buscar datos, " & "\nSentencia:  " & sqlstring)
