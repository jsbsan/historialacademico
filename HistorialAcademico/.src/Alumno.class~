Public Function BuscarMayorQueIdAlumno(valor As Integer, Optional CampoOrden As String) As Result
  
  SQLString = "Select * From Alumno"
  If CampoOrden = Null Then 
    SQlString &= " WHERE idAlumno>" & valor  
  Else 
